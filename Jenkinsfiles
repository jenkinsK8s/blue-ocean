pipeline {
  agent {
    node {
      label 'agent01'
    }

  }
  stages {
    stage('Source') {
      steps {
        git(url: 'https://github.com/******/gradle.git', branch: 'master', credentialsId: '******')
      }
    }

    stage('Build') {
      steps {
        tool 'gradle5'
      }
    }

    stage('Deploy') {
      steps {
        sh 'echo "Deploy Success"'
      }
    }

  }
}
